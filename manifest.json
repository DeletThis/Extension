{
    "name": "Freshify",
    "version": "1.0",
    "description": "Enhancing your browser experience since 2k18",
    "permissions": ["activeTab", "declarativeContent", "storage", "https://*.cloudfunctions.net/"],
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": "images/PepeAngry.png"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "options_page": "options.html",
    "manifest_version": 2
  }